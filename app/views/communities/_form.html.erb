<%= simple_form_for(@community) do |f| %>
  <%= f.error_notification %>
  <div class="field">
    <div class="control">
      <%= f.input :community, presence:true,input_html: { class: 'input' }, wrapper: false, label_html: { class: 'label' } %>
    </div>
  </div>
  <%= f.button :submit, class: 'button is-info' %>
<% end %>
